{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "messageType": {
      "type": "string"
    },
    "publishedDate": {
      "type": "string"
    },
    "tdei_record_id": {
      "type": "string"
    },
    "tdei_org_id": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "stage": {
          "type": "string"
        },
        "request": {
          "type": "object",
          "properties": {
            "tdei_org_id": {
              "type": "string"
            },
            "collected_by": {
              "type": "string"
            },
            "collection_date": {
              "type": "string"
            },
            "collection_method": {
              "type": "string"
            },
            "publication_date": {
              "type": "string"
            },
            "data_source": {
              "type": "string"
            },
            "polygon": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "features": {
                  "type": "array",
                  "items": [
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string"
                        },
                        "geometry": {
                          "type": "object",
                          "properties": {
                            "type": {
                              "type": "string"
                            },
                            "coordinates": {
                              "type": "array",
                              "items": [
                                {
                                  "type": "array",
                                  "items": [
                                    {
                                      "type": "array",
                                      "items": [
                                        {
                                          "type": "number"
                                        },
                                        {
                                          "type": "number"
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            }
                          },
                          "required": [
                            "type",
                            "coordinates"
                          ]
                        }
                      },
                      "required": [
                        "type",
